# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


POST    /json/etch         etch.EtchController.saveEtch()
GET    /json/etch         etch.EtchController.getEtch(latitude: Double, longitude: Double)

# Had to add new endpoints to not break compatibility while rolling out E6 change
POST    /json/etchE6         etch.EtchController.saveEtchE6()
GET    /json/etchE6         etch.EtchController.getEtchE6(latitudeE6: Int, longitudeE6: Int)

# Home page
# GET     /                           controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
# GET     /assets/*file               controllers.Assets.at(path="/public", file)

# GET     /*any                       controllers.Application.deferToAngular(any: String)

